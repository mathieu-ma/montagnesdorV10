<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>

        <class name="fr.montagnesdor.restaurant.model.hibernate.ValueAddedTax" table="t_value_added_tax">
                <id name="id" column="vat_id" type="long">
                        <generator class="increment"/>
                </id>
				<property name="value" column="vat_value" type="float"/>
        </class>

</hibernate-mapping>
