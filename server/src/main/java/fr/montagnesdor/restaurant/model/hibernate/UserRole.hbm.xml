<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
  
        <class name="fr.montagnesdor.restaurant.model.hibernate.UserRole" table="t_user_role">
                <id name="id" column="rol_id" type="long">
                        <generator class="increment"/>
                </id>
				<property name="labelCode" column="rol_label_code"/>
        </class>

</hibernate-mapping>
